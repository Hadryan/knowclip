{
  "name": "audio-flashcard-assistant",
  "productName": "audio-flashcard-assistant",
  "version": "0.4.1",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^3.9.0",
    "@material-ui/icons": "^3.0.1",
    "anki-apkg-export-multi-field": "^0.1.0",
    "asar": "0.14.3",
    "dataurl": "^0.1.0",
    "electron-devtools-installer": "^2.2.4",
    "ffmpeg-static": "^2.4.0",
    "ffprobe-static": "^3.0.0",
    "fluent-ffmpeg": "^2.1.2",
    "material-ui-chip-input": "^1.0.0-beta.14",
    "moment": "^2.24.0",
    "papaparse": "^4.5.0",
    "react": "^16.8.4",
    "react-autosuggest": "^9.4.3",
    "react-dom": "npm:@hot-loader/react-dom",
    "react-hot-loader": "^4.8.0",
    "react-redux": "^7.1.3",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.0",
    "redux-observable": "^1.0.0",
    "redux-persist": "^6.0.0",
    "reselect": "^3.0.1",
    "rxjs": "^6.2.2",
    "subsrt": "^1.0.0",
    "subtitle": "^2.0.3",
    "tempy": "^0.3.0",
    "uuid": "^3.3.2",
    "xss": "^1.0.6"
  },
  "main": "./main.js",
  "homepage": "./",
  "scripts": {
    "start": "react-scripts start",
    "build": "NODE_ENV=production react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "electron": "electron .",
    "lint": "eslint src",
    "format": "prettier --ignore-path ./.gitignore \"{src/**,.}/*.{js,ts,tsx}\"",
    "format:write": "yarn format --write",
    "build-electron:mac": "NODE_ENV=production electron-packager . --overwrite --platform=darwin --arch=x64 --icon=icons/icon.icns --prune=true --out=release-builds --asar.unpackDir=\"**/node_modules/{ffmpeg-static/bin/darwin/x64,ffprobe-static/bin/darwin/x64}*\"",
    "build-electron:linux": "NODE_ENV=production electron-packager . --overwrite --platform=linux --arch=x64 --icon=icons/icon.icns --prune=true --out=release-builds",
    "build-electron:linux-x64": "NODE_ENV=production electron-packager . --overwrite --platform=linux --arch=x64 --icon=icons/icon.icns --prune=true --out=release-builds --asar.unpackDir=\"**/node_modules/{ffmpeg-static/bin/linux/x64,ffprobe-static/bin/linux/x64}*\"",
    "build-electron:win": "electron-packager . audio-flashcard-assistant --asar.unpackDir=\"**/node_modules/{ffmpeg-static/bin/win32,ffprobe-static/bin/win32}*\" --overwrite --platform=win32 --arch=ia32 --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Audio Flashcard Assistant\"",
    "build:mac": "yarn build && yarn build-electron:mac",
    "build:linux": "yarn build && yarn build-electron:linux",
    "build:linux-x64": "yarn build && yarn build-electron:linux-x64",
    "build:win": "yarn build && yarn build-electron:win && zip -r ./release-builds/afca.zip ./release-builds/audio-flashcard-assistant-win32-ia32",
    "deb-amd64": "electron-installer-debian --src release-builds/audio-flashcard-assistant-linux-x64/ --arch amd64 --config debian.json",
    "tsc": "tsc"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn format --list-different && yarn lint && yarn tsc"
    }
  },
  "browserslist": [
    "electron >= 6.0.12"
  ],
  "devDependencies": {
    "@types/fluent-ffmpeg": "^2.1.11",
    "@types/jest": "^24.0.18",
    "@types/node": "^12.7.11",
    "@types/papaparse": "^5.0.1",
    "@types/react": "^16.9.5",
    "@types/react-dom": "^16.9.1",
    "@types/react-redux": "^7.1.4",
    "@types/reselect": "^2.2.0",
    "@types/subtitle": "^2.0.2",
    "@types/uuid": "^3.4.5",
    "@typescript-eslint/eslint-plugin": "^2.3.2",
    "electron": "6.0.12",
    "electron-installer-debian": "^2.0.1",
    "electron-packager": "^14.0.6",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-react-hooks": "^1.6.0",
    "husky": "^1.1.3",
    "prettier": "1.15.3",
    "silvestre-afa-react-scripts": "^3.2.0",
    "typescript": "^3.6.3"
  }
}
